tmp <- read.csv("followers_count.csv")
followers_count <- tmp[,2]
barplot(followers_count,main="Twitter Followers and their number of followers",xlab="Followers sorted by the number of followers they have",ylab="Count of Followers",ylim=c(0,1000))
arrows(x0=match(c(52),followers_count)+12, y0=(max(followers_count) - 80), x1=match(c(52), followers_count)+12, y1=70, length=0.07, lwd=1.5, col='blue')
text(x=match(c(52), followers_count),pos=4,offset=2.5, y=(max(followers_count))-60, labels="dineshpaladhi", col='blue')
